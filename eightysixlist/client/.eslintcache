tion: \"\"\n  })\n\n  const handleChange = (e) => {\n    const { name, value } = e.target;\n    setFormData(prevState => ({\n      ...prevState,\n      [name]: value\n    }))\n  }\n\n  return (\n    <div id=\"register-main-container\">\n    <form onSubmit={(e) => {\n      e.preventDefault();\n      props.handleRegister(formData);\n    }}>\n      <div id=\"input-main-box\">\n      <h3 id=\"register-title\">Register</h3>\n      <div id=\"name-box\">\n        <label>Name: </label>\n          <input\n          className=\"register-input\"\n            type='text'\n            name='name'\n            value={formData.name}\n            onChange={handleChange}\n            autoFocus\n            required\n          />\n       \n      </div>\n      <div id=\"address-box\">\n        <label>Home Address:</label>\n          <input\n          className=\"register-input\"\n            type='text'\n            name='address'\n            value={formData.address}\n            onChange={handleChange}\n            required\n          />\n        \n      </div>\n      <div id=\"email-box\">\n        <label>Email:</label>\n          <input\n          className=\"register-input\"\n            type='text'\n            name='email'\n            value={formData.email}\n            onChange={handleChange}\n            required\n          />\n        \n      </div>\n      <div id=\"username-box\">\n        <label>Username:</label>\n          <input\n          className=\"register-input\"\n            type='text'\n            name='username'\n            value={formData.username}\n            onChange={handleChange}\n            required\n          />\n        \n      </div>\n      <div id=\"password-box\">\n        <label>Password:</label>\n          <input\n          className=\"register-input\"\n            type='password'\n            name='password'\n            value={formData.password}\n            onChange={handleChange}\n            required\n          />\n        \n      </div>\n      <div id=\"confirm-box\">\n        <label>Confirm Password:</label>\n          <input\n          className=\"register-input\"\n            type='password'\n            name='password'\n            value={formData.password}\n            onChange={handleChange}\n            required\n          />\n        \n      </div>\n      <div id=\"phone-box\">\n        <label>Phone Number:</label>\n          <input\n          className=\"register-input\"\n            type='phone'\n            name='phone'\n            value={formData.phone}\n            onChange={handleChange}\n            required\n          />\n      </div>\n      <div id=\"age-box\">\n        <label>Age:</label>\n          <input\n          className=\"register-input\"\n            type='number'\n            name='age'\n            value={formData.age}\n            onChange={handleChange}\n            required\n          />\n        \n      </div>\n      <div id=\"restaurant-box\">\n        <label>I work at...</label>\n          <input\n          className=\"register-input\"\n            type='text'\n            name='restaurant'\n            value={formData.restaurant}\n            onChange={handleChange}\n            required\n          />\n        \n      </div>\n      <div id=\"location-box\">\n        <label >My store is located in...</label>\n          <input\n          className=\"register-input\"\n            type='text'\n            name='location'\n            value={formData.location}\n            onChange={handleChange}\n            required\n          />\n      </div>\n      <div id=\"register-terms-box\">\n          <label id=\"terms-label\">I agree to the <a>Terms and Conditions</a>:\n            <input\n              className=\"login-input\"\n              type='checkbox'\n              name='terms'\n              // value={formData.password}\n              onChange={handleChange}\n              required\n            />\n          </label>\n        </div>\n        <div id=\"register-commu